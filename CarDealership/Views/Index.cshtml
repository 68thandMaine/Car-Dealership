@{
  using CarDealership.Models;
}

<h1>To Do List</h1>
@if (Model.Count == 0)
{
  <p>You have no cars in your inventory!</p>
}
@if(Model.Count != 0)
{
  <ul>
  foreach (Car car in Model)
  {
    <li>@Model.GetMake()</li>
    <li>@Model.GetModel()</li>
    <li>@Model.GetPrice()</li>
    <li>@Model.GetMiles()</li>
    <li>@Model.GetNote()</li>
  }
  </ul>
}

<a href="/items/new">Add a new item.</a>
